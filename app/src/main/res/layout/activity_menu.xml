<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent" tools:layout_editor_absoluteY="81dp"
                                             android:visibility="visible">

    <ImageButton
            android:layout_width="0dp"
            android:layout_height="68dp"
            android:id="@+id/btnBuscarMenu"
            android:visibility="visible"
            android:tint="@color/colorPrimary"
            android:background="@drawable/calendario"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="32dp" app:layout_constraintEnd_toEndOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/textView2"
            android:layout_marginLeft="14dp" app:layout_constraintBottom_toBottomOf="@+id/textView2"
            android:layout_marginStart="14dp" android:layout_marginEnd="14dp" android:layout_marginRight="14dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="64dp"
            android:id="@+id/btnDondeEstoyMenu"
            android:visibility="visible"
            android:tint="@color/colorPrimaryDark"
            android:background="@drawable/ruta"
            android:layout_marginTop="34dp"
            android:layout_marginBottom="32dp" app:layout_constraintEnd_toEndOf="@+id/btnDondeEstoy"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/btnDondeEstoy"
            app:layout_constraintBottom_toBottomOf="@+id/textView5"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="69dp"
            android:id="@+id/btnDondeEstoy"
            android:visibility="visible"
            android:tint="@color/colorPrimaryDark"
            android:background="@drawable/perfil"
            android:layout_marginTop="38dp"
            android:layout_marginBottom="39dp" app:layout_constraintEnd_toEndOf="@+id/textView"
            app:layout_constraintBottom_toTopOf="@+id/btnSalir" app:layout_constraintTop_toTopOf="@+id/textView6"
            app:layout_constraintStart_toStartOf="@+id/textView"/>
    <ImageButton
            android:layout_width="79dp"
            android:layout_height="47dp"
            android:id="@+id/btnSalir"
            android:onClick="Login"
            android:visibility="visible"
            android:tint="@color/colorPrimaryDark"
            android:background="@android:drawable/ic_menu_revert"
            android:layout_marginTop="26dp"
            android:layout_marginBottom="28dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/textView4"/>
    <ImageButton
            android:layout_width="91dp"
            android:layout_height="67dp"
            android:id="@+id/btnMiCalendarioMenu"
            android:visibility="visible"
            android:tint="@color/colorPrimaryDark"
            android:background="@drawable/calendario_login"
            android:layout_marginTop="124dp"
            app:layout_constraintEnd_toStartOf="@+id/btnMiPerfilMenu" android:layout_marginBottom="123dp"
            app:layout_constraintBottom_toTopOf="@+id/txtAcercaDe"
            app:layout_constraintTop_toTopOf="@+id/btnMiPerfilMenu" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="28dp" android:layout_marginStart="28dp" android:layout_marginEnd="30dp"
            android:layout_marginRight="30dp"/>
    <ImageButton
            android:layout_width="87dp"
            android:layout_height="69dp"
            android:id="@+id/btnMiPerfilMenu"
            android:visibility="visible"
            android:tint="@color/colorPrimary"
            android:background="@drawable/mi_perfil"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="@+id/btnDondeEstoyMenu"
            android:layout_marginTop="136dp" android:layout_marginBottom="137dp"
            app:layout_constraintBottom_toTopOf="@+id/textView"/>
    <TextView
            android:text="¿Donde estoy?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="26dp"
            android:layout_marginRight="26dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="116dp"/>
    <TextView
            android:text="¿Que deseo buscar?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            app:layout_constraintTop_toTopOf="@+id/textView5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="13dp" android:layout_marginStart="13dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView5"/>
    <TextView
            android:text="Mis Horarios :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="@+id/btnMiCalendarioMenu" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnMiCalendarioMenu"/>
    <TextView
            android:text="Salir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="@+id/btnSalir" android:layout_marginLeft="25dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="25dp"
            android:layout_marginBottom="34dp"/>
    <TextView
            android:text="¿Donde debo ir hoy?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="111dp" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Mi Perfil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@+id/btnMiPerfilMenu"/>
    <TextView
            android:text="Acerca De La Aplicación"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/txtAcercaDe"
            android:textAppearance="@style/TextAppearance.AppCompat.Title" android:visibility="visible"
            android:autoLink="web" android:capitalize="sentences" android:clickable="true"
            android:textColor="@color/colorPrimary" android:textColorHighlight="@color/colorPrimaryDark"
            android:textSize="14sp" android:textStyle="normal|bold|italic" android:typeface="normal"
            android:verticalScrollbarPosition="defaultPosition"
            android:layout_marginTop="7dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>